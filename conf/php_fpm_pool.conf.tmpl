[{{ .ProjectName }}_{{ .AppName }}]
user = {{ .User }}
listen = {{ .Socket }}
pm = dynamic
pm.max_children = 5
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 3

{{ range $key, $val := .Env }}{{ if (ne $val "") }}env[{{ $key }}] = "{{ $val }}"
{{ end }}{{ end }}
{{ range $key, $val := .Ini }}{{ if (ne $val "") }}php_admin_value[{{ $key }}] = "{{ $val }}"
{{ end }}{{ end }}