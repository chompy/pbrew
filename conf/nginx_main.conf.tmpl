worker_processes  1;
pid        {{ .Pid }};
error_log  {{ .ErrorLog }};

events {
    worker_connections  1024;
}

http {
    include                 mime.types;
    default_type            application/octet-stream;
    access_log              {{ .AccessLog }}  main;
    sendfile                on;
    keepalive_timeout       65;
    #gzip  on;
    include servers/*;
}